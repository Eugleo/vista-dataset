  "@type": "sc:Dataset",
  "name": "ViPR",
  "conformsTo": "http://mlcommons.org/croissant/1.0",
  "distribution": [
    {
      "@type": "cr:FileObject",
      "@id": "video_file",
      "name": "video_file",
      "description": "A JSON file containing the video metadata, including labels and descriptions.",
      "contentUrl": "./dataset.json",
      "encodingFormat": "text/json",
      "sha256": "6c27671e1c89d08188dbdf45c24e3b443c3ba8c8f1cadafadbd23df567d472f8"
    },
    {
      "@type": "cr:FileObject",
      "@id": "video_archive",
      "description": "A zip file containing all the videos",
      "contentUrl": "https://storage.googleapis.com/vipr/vlm_benchmark_videos.zip",
      "encodingFormat": "application/zip",
      "sha256": "0945a8d60926892d9be67d7a3dc0a07dbd43cdecc1ad5832fa8712c2b70fa580"
    },
    {
      "@type": "cr:FileSet",
      "@id": "video_mp4",
      "containedIn": {
        "@id": "video_archive"
      },
      "encodingFormat": "video/mp4",
      "includes": "**/*.mp4"
    }
  ],
  "recordSet": [
    {
      "@type": "cr:RecordSet",
      "@id": "video_metadata",
      "name": "record_set",
      "field": [
        {
          "@type": "cr:Field",
          "@id": "video_metadata/video",
          "name": "video",
          "description": "Path to a video file",
          "dataType": "sc:Text",
          "source": {
            "fileObject": {
              "@id": "video_file"
            },
            "extract": {
              "jsonPath": "$[*].video"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "video_metadata/n_steps",
          "name": "n_steps",
          "description": "Number of actions in the video",
          "dataType": "sc:Integer",
          "source": {
            "fileObject": {
              "@id": "video_file"
            },
            "extract": {
              "jsonPath": "$[*].n_steps"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "video_metadata/environment",
          "name": "environment",
          "description": "The environment the video was taken in (minecraft, virtual home, real life)",
          "dataType": "sc:Text",
          "source": {
            "fileObject": {
              "@id": "video_file"
            },
            "extract": {
              "jsonPath": "$[*].environment"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "video_metadata/group",
          "name": "group",
          "description": "The semantic problem group",
          "dataType": "sc:Text",
          "source": {
            "fileObject": {
              "@id": "video_file"
            },
            "extract": {
              "jsonPath": "$[*].group"
            }
          }
        },
        {
          "@type": "cr:Field",
          "@id": "video_metadata/labels",
          "name": "labels",
          "description": "List of labels of this video",
          "dataType": "sc:Text",
          "source": {
            "fileObject": {
              "@id": "video_file"
            },
            "extract": {
              "jsonPath": "$[*].labels"
            }
          },
          "subField": [
            {
              "@type": "cr:Field",
              "@id": "video_metadata/labels/task",
              "name": "labels/task",
              "description": "The task this label belongs to",
              "dataType": "sc:Text",
              "source": {
                "fileObject": {
                  "@id": "video_file"
                },
                "extract": {
                  "jsonPath": "$[*].task"
                }
              }
            },
            {
              "@type": "cr:Field",
              "@id": "video_metadata/labels/label",
              "name": "labels/label",
              "description": "The label id this label object describes",
              "dataType": "sc:Text",
              "source": {
                "fileObject": {
                  "@id": "video_file"
                },
                "extract": {
                  "jsonPath": "$[*].label"
                }
              }
            },
            {
              "@type": "cr:Field",
              "@id": "video_metadata/labels/description",
              "name": "labels/description",
              "description": "The natural language description of this label",
              "dataType": "sc:Text",
              "source": {
                "fileObject": {
                  "@id": "video_file"
                },
                "extract": {
                  "jsonPath": "$[*].description"
                }
              }
            }
          ]
        }
      ]
    },
    {
      "@type": "cr:RecordSet",
      "@id": "videos",
      "name": "record_set_video",
      "field": [
        {
          "@type": "cr:Field",
          "@id": "videos/id",
          "name": "video_id",
          "description": "video path",
          "dataType": "sc:Text",
          "source": {
            "fileSet": {
              "@id": "video_mp4"
            },
            "extract": {
              "fileProperty": "filepath"
            }
          },
          "references": {
            "@id": "video_metadata/video"
          }
        },
        {
          "@type": "cr:Field",
          "@id": "videos/content",
          "name": "video_content",
          "description": "The mp4 video itself",
          "dataType": "sc:ImageObject",
          "source": {
            "fileSet": {
              "@id": "video_mp4"
            },
            "extract": {
              "fileProperty": "content"
            }
          }
        }
      ]
    }
  ]
}